public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
                int t = sc.nextInt();
                while(t-- > 0){
                        long n = sc.nextLong();
                        System.out.println(flip(n));
                }
	}
        public static long flip(long n)
        {
                long l = 4294967295L;
                return l - n;
        }
}
